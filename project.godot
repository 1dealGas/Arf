config_version=5

[application]

config/name="Arf"
run/main_scene="res://〈Arf〉.tscn"
run/disable_stderr=true
config/features=PackedStringArray("4.2", "GL Compatibility")
run/flush_stdout_on_print.debug=false
run/low_processor_mode=true
run/low_processor_mode_sleep_usec=6000
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://include/wish.png"

[audio]

driver/driver=""

[debug]

file_logging/enable_file_logging.pc=false
settings/gdscript/max_call_stack=512
gdscript/warnings/static_called_on_instance=2

[display]

